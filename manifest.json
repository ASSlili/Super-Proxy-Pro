{
  "manifest_version": 3,
  "name": "Super Proxy Switcher Pro",
  "version": "2.0",
  "description": "全功能代理管理工具：支持认证、导入导出、IP检测。",
  "permissions": [
    "proxy",
    "storage",
    "webRequest",
    "webRequestAuthProvider"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Proxy Switcher"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.ico"
  }
}